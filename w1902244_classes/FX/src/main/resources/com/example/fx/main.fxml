<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fx.MainController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#bcc8d4" height="140.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#e2e6ea" height="260.0" layoutY="140.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
      <Label layoutX="121.0" layoutY="37.0" text="Welcome To Fuel Queue Viewer">
         <font>
            <Font name="Onyx" size="48.0" />
         </font>
      </Label>
      <Button layoutX="211.0" layoutY="192.0" mnemonicParsing="false" onAction="#viewDetails" prefHeight="42.0" prefWidth="174.0" text="View  All Queue Details">
         <font>
            <Font name="Georgia" size="14.0" />
         </font>
      </Button>
      <Button layoutX="213.0" layoutY="259.0" mnemonicParsing="false" onAction="#searchPassenger" prefHeight="42.0" prefWidth="174.0" text="Search Passenger">
         <font>
            <Font name="Georgia" size="14.0" />
         </font>
      </Button>
      <Button layoutX="490.0" layoutY="355.0" mnemonicParsing="false" onAction="#quit" prefHeight="30.0" prefWidth="96.0" text="Quit">
         <font>
            <Font name="Georgia" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
